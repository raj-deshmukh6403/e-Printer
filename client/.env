# Client environment variables
REACT_APP_API_URL=https://e-printer-rouge.vercel.app/api
REACT_APP_SOCKET_URL=https://e-printer-rouge.vercel.app/
REACT_APP_APP_NAME=E-Printer
REACT_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=production
NODE_ENV=production

# Payment Gateway (Razorpay)
REACT_APP_RAZORPAY_KEY_ID=your_razorpay_key_id
REACT_APP_RAZORPAY_KEY=your_razorpay_key_here

# File Upload Settings
REACT_APP_MAX_FILE_SIZE=52428800
REACT_APP_ALLOWED_FILE_TYPES=application/pdf

# WebSocket for real-time updates
REACT_APP_WEBSOCKET_URL=ws://localhost:5000

# Other settings
REACT_APP_ENABLE_PWA=true
REACT_APP_ENABLE_ANALYTICS=false